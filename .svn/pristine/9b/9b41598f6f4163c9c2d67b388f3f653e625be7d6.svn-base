.charge-but-div li .green-but{ width:80px;}
.charge-but-div li .green-font{font-size:12px;}
.charge-but-div li .yellow-but{ padding:0 15px; color:#fff;}
.name-table{position:absolute; height:100px; width:300px; overflow-y:auto; z-index:900; background:#fff; border:1px solid #cdcdcd; line-height:26px; margin-left:80px; margin-top:-8px; font-size:12px;}
.name-table table{ width:100%;}
.name-table td{ border-bottom:1px dashed #cdcdcd; cursor:pointer;}
/*门诊开单*/
.middle-tab{width:958px; height:36px; border:1px solid #e1e1e1; background:#f9f9f9; position:relative;}
.middle-tab span{ position:absolute; top:-1px;}
.middle-tab a{ display:inline-block; height:36px; line-height:36px; text-align:center; width:100px; border-top:1px solid #e1e1e1; border-right:1px solid #e1e1e1; border-bottom:1px solid #e1e1e1; color:#333;}
.middle-tab a:hover,.middle-tab .now{ background:#fff; color:#309300;}
.middle-tab .now{ border-top:1px solid #309300; border-bottom:1px solid #fff; }
.w958-border{width:958px; border:1px solid #e1e1e1;}
