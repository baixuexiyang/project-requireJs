.close{ background:url(../images/open.png) 0 -18px no-repeat; width:18px; height:18px; margin-top:2px;}
.bill-list-save{ width:948px;}
.bill-list-save span i{width:50px; height:36px; vertical-align:top;}
.bill-list-save span i,.bill-list-save span label{ display:inline-block;}
.bill-list-save span label textarea{ width:890px; padding:0 3px; line-height:18px; height:36px; border:1px solid #cdcdcd; overflow:hidden;}
.print-area{display:none;}
/*辅助治疗*/
.help-doctor-r{width:810px;}
.help-doctor-r li{ width:270px;}
.help-doctor-r .three-list{ padding-top:0;}
.help-doctor-r .three-list .w215{ width:165px;}
.help-doctor-r .three-list .w415{ width:415px;}
.help-doctor-r .three-list .w415 label{ width:320px;}
.help-doctor-r .textarea-li textarea{ width:695px;}
.help-doctor-r .add-family-but{ width:790px;}
.help-doc-r-t li{ padding-top:4px; padding-bottom:5px; width:232px;}
.help-relation-name{width:170px; height:26px; line-height:26px; padding:0 5px;  border:1px solid #e1e1e1; margin:0 auto; overflow:hidden; border-radius:4px;}
.help-relation-name label input{ width:100%; border:0; height:24px; line-height:24px;}
.table-list td .help-relation-name a{ width:22px; height:26px; background:url(../images/seach-ico.gif); margin:0;}
.help-doctor-r-chack { padding:10px;}
.help-doctor-r-chack span{ padding-right:40px;}
.help-doctor-r-chack span i{ color:#555;}
.help-doctor-r-chack span label{ padding-right:10px;}
.help-doctor-r-chack span a:hover{ text-decoration:underline;}
.help-seach-people{width:400px; height:150px; margin-top:-8px; margin-left:42px; position:absolute; border:1px solid #e1e1e1; z-index:900; overflow-x:hidden; overflow-y:auto;}
.help-seach-people tr,.add-ylfa-list h3{ cursor:pointer;}
.add-ylfa-list .hov-bg-green,.add-ylfa-list .hov-bg-green:hover{ border:1px solid #a7dd8f; background:url(../images/tree-b.gif) 10px center no-repeat #d8f6c9;}
.add-ylfa-list .bg-f5{ border:1px solid #e1e1e1; background:url(../images/tree-a.gif) 10px center no-repeat #f5f5f5;}
.add-ylfa-list h3{ text-indent:28px;}
.add-ylfa-list h3:hover{ border:1px solid #a7dd8f; background:url(../images/tree-a.gif) 10px center no-repeat #d8f6c9;}
.add-ylfa-hov{ height:36px; line-height:36px; display:block; font-weight:bold;}
.add-ylaf-ul li{ line-height:36px; padding-bottom:10px;}
.add-ylaf-ul cite{ display:inline-block; width:180px; text-align:right;}
.add-ylaf-ul li input.w-160{ height:30px; height:30px;}
.add-ylaf-ul li textarea{ width:560px; border:1px solid #e1e1e1; border-radius:4px; font-family:"微软雅黑"; font-size:14px; line-height:20px; height:80px; padding:2px;}
.v-a-t{vertical-align:top;}
.add-ylaf-ul li label{ margin-right:10px;}
.add-lcfa .green-but{width:160px; margin-left:10px;}
.add-lcfa p span{ padding-right:30px;}
.add-lcfa-tab td blue-but{ width:52px; padding:0;}
.add-lcfa-tab .text-l i{ margin-left:10px;}
.do-event a{ display:inline-block; margin-right:10px; width:68px; height:22px; line-height:22px; border:1px solid #309300; text-align:center; border-radius:4px; color:#309300;}
.do-event a:hover,.do-event .now{ background:#309300; color:#fff;}
/*精液基础分析*/
.jy-fx-title{ line-height:30px;}
.jy-fx-title cite{ display:block;}
.jy-fx-title span{ padding-right:20px;}
.jy-fx-title span input,.jy-fx-content li input{ height:22px; line-height:22px; border:1px solid #cdcdcd; padding:0 2px; border-radius:4px; font-family:"微软雅黑";font-size:14px;}
.jy-fx-content{ width:938px; padding:15px 10px; border:1px solid #e1e1e1;}
.jy-fx-content li{ width:469px; height:30px; line-height:30px; padding-bottom:10px; float:left;}
.jy-fx-content li i{ display:inline-block; width:160px; text-align:right;}
.jy-fx-content li input,.jy-fx-content li select{ width:215px;}
.jy-fx-bottom i{ vertical-align:top; display:inline-block; width:50px;}
.jy-fx-bottom textarea{ width:902px; border:1px solid #e1e1e1; line-height:20px; font-family:"微软雅黑";font-size:14px; padding:2px; height:80px;}