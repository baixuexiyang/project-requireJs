					$.ajax(common.ajaxCall({
						url: api,
						type: 'post',
						success: function() {
							art.dialog({
								content: '操作成功！',
								icon: 'success',
								time: 1
							});
							setTimeout(function() {
								art.dialog.close();
								top.location.reload();
							}, 1100);
						},
						error: function(msg) {
							alert(msg);
						}
					}));

				};
				$(this).dialogConfirm(callback);
			});

		}
	};

